<div class="container">

<table class="table table-striped">
<th colspan="6" id="ritly">Ritly DB <h4 id="flash"><%= flash[:warning] %></h4></th>
<tr>
<td>ID</td>
<td>Original URL</td>
<td>String URL</td>
<td>Created</td>
<td>Updated</td>
<% @db.each do |db| %>
<tr>
<td><%= db["id"] %></td>
<td><%= db["og_url"] %></td>
<td><%= db["new_url"] %></td>
<td><%= db["created_at"] %></td>
<td><%= db["updated_at"] %></td>
<td><%= button_to "delete", {controller: :urls, action: :delete, id: db["id"] }, method: :delete, form_class: "btn btn-danger btn-sm" %></td>

</tr>
<% end %>

</table>

</div>